<AnchorPane prefHeight="387.0" prefWidth="590.0" style="-fx-background-color: #7ac17a;" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.ui.LoginUserController">
   <children>
      <Label alignment="CENTER" contentDisplay="CENTER" layoutX="28.0" layoutY="26.0" prefHeight="95.0" prefWidth="606.0" text="Welcome to login user System !!">
         <font>
            <Font name="Perpetua Titling MT Bold" size="16.0" />
         </font>
         <opaqueInsets>
            <Insets />
