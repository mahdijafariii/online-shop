<AnchorPane prefHeight="590.0" prefWidth="646.0" style="-fx-background-color: #7ac17a;" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.ui.LoginUserController">
   <children>
      <Label alignment="CENTER" contentDisplay="CENTER" layoutX="28.0" layoutY="26.0" prefHeight="95.0" prefWidth="606.0" text="Welcome to login user System !!">
         <font>
            <Font name="Perpetua Titling MT Bold" size="16.0" />
         </font>
         <opaqueInsets>
            <Insets />
         </opaqueInsets>
      </Label>
      <Label alignment="CENTER" contentDisplay="CENTER" layoutX="184.0" layoutY="133.0" prefHeight="26.0" prefWidth="74.0" text="User name:" />
      <Label alignment="CENTER" layoutX="190.0" layoutY="180.0" prefHeight="26.0" prefWidth="60.0" text="password:" />
      <TextField fx:id="UserNameLogin" layoutX="276.0" layoutY="133.0" />
      <PasswordField fx:id="PasswordLogin" layoutX="276.0" layoutY="180.0" />
      <Button fx:id="loginInLoginSystem" layoutX="219.0" layoutY="239.0" mnemonicParsing="false" onAction="#checkUserLoginSystem" text="Login" />
      <Button fx:id="deleteFieldsOfLoginSystem" layoutX="307.0" layoutY="238.0" mnemonicParsing="false" onAction="#deleteFieldsOfLoginSystem" text="delete" />
      <Button fx:id="backFromLoginSystem" layoutX="399.0" layoutY="238.0" mnemonicParsing="false" onAction="#backFromLogin" text="back" />
   </children>
   <cursor>
      <Cursor fx:constant="DEFAULT" />
   </cursor>
   <padding>
      <Insets bottom="25.0" left="25.0" right="25.0" top="300.0" />
   </padding>
</AnchorPane>
